    {
      "name": "global",
      "scriptText": `# BLUEPRINT global
# TAG IsCharControllable false
# TAG IsPozyxControllable false
# TAG IsPtrackControllable false

# PROGRAM INIT
addFeature IU
addFeature Population
addFeature Timer

prop global.visible setTo false

addProp noticingString string ' '
addProp tempString string ' '

// required directive
# PROGRAM UPDATE

onEvent START [[
  featCall global.IU logString 'Starting Mammoth Model'
]]`
    },
    {
      "name": "Tree",
      "scriptText": `# BLUEPRINT Tree
# TAG isCharControllable false
# TAG isPozyxControllable false
# TAG isPTrackControllable false

# PROGRAM INIT
addFeature Costume
addFeature Physics
addFeature Touches
addFeature Graphing
addFeature Population

addProp populationCount number 50
prop Tree.populationCount setMax 100
prop Tree.populationCount setMin 0
featProp Tree.Graphing.meterProp setTo 'populationCount'

featProp Tree.Costume.costumeName setTo 'WL_tree.png'
featProp Tree.Costume.scale setTo 0.25
prop Tree.zIndex setTo 100


# PROGRAM UPDATE

every 1 runAtStart [[
  exprPush {{ 'Trees (' + Tree.getProp('populationCount').value + ')' }}
  featPropPop Tree.Graphing.text

  ifProp Tree.populationCount equal 0 [[
    // ‚úèÔ∏è Make a popup to tell show users that this beaver was eaten!
    featCall Tree.Population createCharacter PopupEffect [[
      prop PopupEffect.textToUse setTo 'No more trees! You lose.'
      prop PopupEffect.costumeToUse setTo ''
      featCall global.Timer stopRound
    ]]

  ]]

]]`
    },
    {
      "name": "Mammoth",
      "scriptText": `# BLUEPRINT Mammoth
# TAG isPozyxControllable false
# TAG isCharControllable false
# TAG isPTrackControllable false

// üîé HOW This is the script that sets up how the mammoths in our model work.
// üîé HOW How do you think being a Mammoth changes the way you understand their environment?

# PROGRAM INIT

addFeature Costume
addFeature Physics
addFeature Touches
addFeature Graphing
addFeature Movement
addFeature IU
addFeature Population

addProp populationCount number 0
prop Mammoth.populationCount setMax 100
prop Mammoth.populationCount setMin 0
featProp Mammoth.Graphing.meterProp setTo 'populationCount'

featProp Mammoth.Costume.costumeName setTo 'MM_mammoth.png'
featProp Mammoth.Costume.scale setTo 0.3
prop Mammoth.zIndex setTo 300


# PROGRAM UPDATE

every 1 runAtStart [[
  exprPush {{'Mammoths (' + Mammoth.getProp('populationCount').value + ')'}}
  featPropPop Mammoth.Graphing.text
]]

every 5 runAtStart [[
  ifProp populationCount greaterThan 0 [[
    // eat some Trees
    featCall global.Population tellCharacterByName Trees [[
      prop character.populationCount sub 1
    ]]
    // glow the arrow to show we ate the trees 
    featCall global.Population tellCharacterByName MammothToTree [[
      featProp character.Costume.glow setTo 1
    ]]
  ]]

  // if more mammoths, eat more
  ifProp populationCount greaterThan 5 [[
    // eat some Trees
    featCall global.Population tellCharacterByName Trees [[
      prop character.populationCount sub 1
    ]]
  ]]

  // if a lot of mammoths, eat more
  ifProp populationCount greaterThan 10 [[
    // eat some Trees
    featCall global.Population tellCharacterByName Trees [[
      prop character.populationCount sub 1
    ]]
  ]]

]]

`
    },
    {
      "name": "HunterGatherer",
      "scriptText": `# BLUEPRINT HunterGatherer
# TAG isCharControllable false
# TAG isPozyxControllable false
# TAG isPTrackControllable false

# PROGRAM INIT
addFeature Costume
addFeature Physics
addFeature Touches
addFeature Movement
addFeature IU
addFeature Graphing

addProp populationCount number 0
prop HunterGatherer.populationCount setMax 100
prop HunterGatherer.populationCount setMin 0
featProp HunterGatherer.Graphing.meterProp setTo 'populationCount'

featProp HunterGatherer.Costume.costumeName setTo 'MM_meeple.png'
featProp HunterGatherer.Costume.color setToColor 4084985
featProp HunterGatherer.Costume.scale setTo 0.1
prop HunterGatherer.zIndex setTo 410


# PROGRAM UPDATE

every 1 runAtStart [[
  exprPush {{'HunterGatherers (' + HunterGatherer.getProp('populationCount').value + ')'}}
  featPropPop HunterGatherer.Graphing.text
]]

every 5 runAtStart [[
  ifProp populationCount greaterThan 0 [[
      // eat some Trees
      featCall global.Population tellCharacterByName Trees [[
        prop character.populationCount sub 1
      ]]

      // glow the arrow to show we ate the trees 
      featCall global.Population tellCharacterByName HunterToTree [[
        featProp character.Costume.glow setTo 1
      ]]
    ]]

  ifProp populationCount greaterThan 5 [[

      featCall global.Population tellCharacterByName Trees [[
        prop character.populationCount sub 1
      ]]

      featCall global.Population tellCharacterByName Mammoths [[
        prop character.populationCount sub 1
      ]]

      // glow the arrow to show we ate the Mammoths 
      featCall global.Population tellCharacterByName HunterToMammoth [[
        featProp character.Costume.glow setTo 1
      ]]

    ]]

      ifProp populationCount greaterThan 10 [[
        featCall global.Population tellCharacterByName Trees [[
          prop character.populationCount sub 1
        ]]

        featCall global.Population tellCharacterByName Mammoths [[
          prop character.populationCount sub 1
        ]]
      ]]
  ]]


`
